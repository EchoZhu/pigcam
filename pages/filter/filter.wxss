/* pages/nomo/nomo.wxss */

page {
  background-color: #fff;
}

.operator-area-container {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 58px;
  margin-bottom: 20px;
}

.operator-area-filter {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding-right: 30px;
  padding-left: 30px;
}

.operator-area-filter image {
  width: 50px;
  height: 50px;
  border: 0 solid #ccc;
}

.operator-area {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding-right: 30px;
  padding-left: 30px;
}

.save {
  width: 50px;
  height: 50px;
  border: 0 solid #ccc;
}

.chosefilter {
  width: 50px;
  height: 50px;
  border: 0 solid #ccc;
}

.share::after {
  border: none;
}

.share {
  width: 58px;
  height: 58px;
  background-size: 58px 58px;
  border: 0 solid #fff;
  background-repeat: no-repeat; /** 不重复*/
  background-color: #fff;
  background-image: url('data:image/png;base64,');
}
